        "description": "Analyze file path and return metadata",
        "priority": 0,
        "timeout_ms": 5000,
        "parameters": [
          {
            "name": "path",
            "type": "str",
            "required": true,
            "description": "File path to analyze"
          }
        ],
        "return_type": "dict",
        "tags": ["file", "utility"],
        "version": "1.0.0"
      },
      {
        "name": "batch_execute",
        "function": "batch_execute",
        "command_id": "batch.execute",
        "description": "Execute a batch of operations",
        "priority": 10,
        "timeout_ms": 30000,
        "parameters": [
          {
            "name": "operations",
            "type": "list",
            "required": true,
            "description": "List of operation dictionaries with 'type' and 'args'"
          }
        ],
        "return_type": "dict",
        "tags": ["batch", "processing"],
        "version": "1.0.0"
      },
      {
        "name": "echo",
        "function": "echo",
        "command_id": "util.echo",
        "description": "Echo a message back (command version)",
        "priority": 0,
        "timeout_ms": 1000,
        "parameters": [
          {
            "name": "message",
            "type": "str",
            "required": true,
            "description": "Message to echo"
          },
          {
            "name": "prefix",
            "type": "str",
            "required": false,
            "default": "",
            "description": "Optional prefix to add"
          }
        ],
        "return_type": "dict",
        "tags": ["utility"],
        "version": "1.0.0"
      }
    ]
  }
}
