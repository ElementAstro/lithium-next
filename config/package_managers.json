
{
    "package_managers": [
        {
            "name": "apt",
            "check_cmd": "dpkg -l {}",
            "install_cmd": "sudo apt-get install -y {}",
            "uninstall_cmd": "sudo apt-get remove -y {}",
            "search_cmd": "apt-cache search {}"
        },
        {
            "name": "dnf",
            "check_cmd": "rpm -q {}",
            "install_cmd": "sudo dnf install -y {}",
            "uninstall_cmd": "sudo dnf remove -y {}",
            "search_cmd": "dnf search {}"
        },
        {
            "name": "pacman",
            "check_cmd": "pacman -Qs {}",
            "install_cmd": "sudo pacman -S --noconfirm {}",
            "uninstall_cmd": "sudo pacman -R --noconfirm {}",
            "search_cmd": "pacman -Ss {}"
        },
        {
            "name": "zypper",
            "check_cmd": "rpm -q {}",
            "install_cmd": "sudo zypper install -y {}",
            "uninstall_cmd": "sudo zypper remove -y {}",
            "search_cmd": "zypper search {}"
        },
        {
            "name": "flatpak",
            "check_cmd": "flatpak list | grep {}",
            "install_cmd": "flatpak install -y {}",
            "uninstall_cmd": "flatpak uninstall -y {}",
            "search_cmd": "flatpak search {}"
        },
        {
            "name": "snap",
            "check_cmd": "snap list {}",
            "install_cmd": "sudo snap install {}",
            "uninstall_cmd": "sudo snap remove {}",
            "search_cmd": "snap find {}"
        },
        {
            "name": "brew",
            "check_cmd": "brew list {}",
            "install_cmd": "brew install {}",
            "uninstall_cmd": "brew uninstall {}",
            "search_cmd": "brew search {}"
        },
        {
            "name": "port",
            "check_cmd": "port installed {}",
            "install_cmd": "sudo port install {}",
            "uninstall_cmd": "sudo port uninstall {}",
            "search_cmd": "port search {}"
        },
        {
            "name": "choco",
            "check_cmd": "choco list --local-only {}",
            "install_cmd": "choco install {} -y",
            "uninstall_cmd": "choco uninstall {} -y",
            "search_cmd": "choco search {}"
        },
        {
            "name": "scoop",
            "check_cmd": "scoop list {}",
            "install_cmd": "scoop install {}",
            "uninstall_cmd": "scoop uninstall {}",
            "search_cmd": "scoop search {}"
        },
        {
            "name": "winget",
            "check_cmd": "winget list {}",
            "install_cmd": "winget install -e --id {}",
            "uninstall_cmd": "winget uninstall -e --id {}",
            "search_cmd": "winget search {}"
        }
    ]
}
